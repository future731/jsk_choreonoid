  "BodyMotionEngine": 
    updateJointVelocities: false
  "EditableSceneBody": 
    editableSceneBodies: 
      - 
        bodyItem: 2
        showCenterOfMass: false
        showPpcom: true
        showZmp: true
      - 
        bodyItem: 7
        showCenterOfMass: false
        showPpcom: false
        showZmp: false
